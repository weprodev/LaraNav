{
  "name": "weprodev/laranav",
  "description": "LaraNav will manage navigation app",
  "license": "MIT",
  "type": "project",
  "homepage": "https://weprodev.com/docs/laranav",
  "keywords": [
    "Project",
    "LaraNav",
    "laraval",
    "Laravel Navigation",
    "Navigation",
    "laravel menu",
    "Laravel menubar"
  ],
  "autoload": {
    "psr-4": {
      "WeProDev\\LaraNav\\": "src/"
    }
  },
  "authors": [
    {
      "name": "WeProDev Team",
      "email": "info@weprodev.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "autoload-dev": {
    "psr-4": {
      "WeProDev\\LaraNav\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint": "php artisan pint"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "laravel/pint": "^1.14",
    "phpstan/phpstan": "^1.10",
    "larastan/larastan": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "WeProDev\\LaraNav\\LaraNavProvider"
      ]
    }
  }
}
